      <akn:section eId="sec_5_331_02">
        <akn:num>5-331.02</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_5-331_02_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“First Amendment assembly” means a demonstration, rally, parade, march, picket line, or other similar gathering conducted for the purpose of persons expressing their political, social, or religious views.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_5-331_02_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“MPD” means the Metropolitan Police Department.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
