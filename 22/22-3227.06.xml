      <akn:section eId="sec_22_3227_06">
        <akn:num>22-3227.06</akn:num>
        <akn:heading>Jurisdiction.</akn:heading>
<akn:subsection eId="subsec_22-3227_06_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>The person whose personal identifying information is improperly obtained, created, possessed, or used is a resident of, or located in, the District of Columbia; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-3227_06_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Any part of the offense takes place in the District of Columbia.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
