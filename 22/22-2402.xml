      <akn:section eId="sec_22_2402">
        <akn:num>22-2402</akn:num>
        <akn:heading>Perjury.</akn:heading>
<akn:subsection eId="subsec_22-2402_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>A person commits the offense of perjury if:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-2402_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>Any person convicted of perjury shall be fined not more than the amount set forth in ยง 22-3571.01 or imprisoned for not more than 10 years, or both.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
