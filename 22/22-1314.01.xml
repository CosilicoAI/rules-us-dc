      <akn:section eId="sec_22_1314_01">
        <akn:num>22-1314.01</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_22-1314_01_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Health professional” means a person licensed to practice a health occupation in the District pursuant to § 3-1201.01.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-1314_01_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Medical facility” includes a hospital, clinic, physician’s office, or other facility that provides health or surgical services.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-1314_01_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Person” shall not include:</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
