      <akn:section eId="sec_22_2723">
        <akn:num>22-2723</akn:num>
        <akn:heading>Property subject to seizure and forfeiture.</akn:heading>
<akn:subsection eId="subsec_22-2723_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>The following are subject to forfeiture:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-2723_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>All seizures and forfeitures of property under this section shall be pursuant to the standards and procedures set forth in D.C. Law 20-278.</akn:p></akn:content>
    </akn:subsection>
