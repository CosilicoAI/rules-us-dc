      <akn:section eId="sec_22_3218_01">
        <akn:num>22-3218.01</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_22-3218_01_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Company” means a person or enterprise engaged in the generation or distribution of natural gas or electricity.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-3218_01_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Person” means any individual, corporation, company, association, firm, partnership, joint stock company, or other entity.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
