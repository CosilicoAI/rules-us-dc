      <akn:section eId="sec_22_3020_51">
        <akn:num>22-3020.51</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_22-3020_51_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Child” means an individual who has not yet attained the age of 16 years.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-3020_51_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Person” means an individual 18 years of age or older.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-3020_51_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Police” means the Metropolitan Police Department.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_22-3020_51_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>“Sexual abuse” means any act that is a violation of:</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
