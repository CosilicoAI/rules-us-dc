      <akn:section eId="sec_47_3151">
        <akn:num>47-3151</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_47-3151_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Drawer” means an individual who makes or signs a check or other draft, but not including a credit or debit card sales draft.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-3151_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Sale” means any:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-3151_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Service” means any:</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
