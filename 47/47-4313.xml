      <akn:section eId="sec_47_4313">
        <akn:num>47-4313</akn:num>
        <akn:heading>Examinations of books and records.</akn:heading>
<akn:subsection eId="subsec_47-4313_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Examine any books, papers, records, or other data which may be relevant or material to such inquiry; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-4313_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Take such testimony of the person concerned, under oath, as may be relevant to such inquiry.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
