      <akn:section eId="sec_47_1005_02">
        <akn:num>47-1005.02</akn:num>
        <akn:heading>Nonprofit affordable housing developer tax relief.</akn:heading>
<akn:subsection eId="subsec_47-1005_02_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-1005_02_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-1005_02_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>The grant of a tax exemption as provided in this section shall be in addition to, and not in lieu of, any other tax relief or assistance from any other source applicable to either the real property or its owner.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-1005_02_d">
      <akn:num>(d)</akn:num>
      <akn:content><akn:p>This section shall apply for real property tax years beginning after September 30, 2012.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
