      <akn:section eId="sec_47_2714">
        <akn:num>47-2714</akn:num>
        <akn:heading>District of Columbia Village rates.</akn:heading>
<akn:subsection eId="subsec_47-2714_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>The per diem rate to be charged for skilled care patients at District of Columbia Village shall be $82.50.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-2714_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>The per diem rate to be charged for intermediate care patients at District of Columbia Village shall be $56.00.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
