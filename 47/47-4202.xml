      <akn:section eId="sec_47_4202">
        <akn:num>47-4202</akn:num>
        <akn:heading>Interest on overpayments.</akn:heading>
<akn:subsection eId="subsec_47-4202_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Unless otherwise provided in this title, interest shall be allowed and paid on an overpayment of a tax imposed by this title at the overpayment rate set forth in subsection (c) of this section.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_47-4202_b">
