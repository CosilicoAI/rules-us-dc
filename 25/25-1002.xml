      <akn:section eId="sec_25_1002">
        <akn:num>25-1002</akn:num>
        <akn:heading>Purchase, possession or consumption by persons under 21; misrepresentation of age; penalties.</akn:heading>
<akn:subsection eId="subsec_25-1002_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>No person who is under 21 years of age shall purchase, attempt to purchase, possess, or drink an alcoholic beverage in the District, except as provided under subchapter IX of Chapter 7.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_25-1002_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_25-1002_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_25-1002_d">
      <akn:num>(d)</akn:num>
      <akn:content><akn:p>Repealed.</akn:p></akn:content>
