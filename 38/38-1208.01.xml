        <akn:num>38-1208.01</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_38-1208_01_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Repealed.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_38-1208_01_2">
