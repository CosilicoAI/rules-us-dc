      <akn:section eId="sec_38_755_02">
        <akn:num>38-755.02</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_38-755_02_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Chancellor” means the chief executive officer of the District of Columbia Public Schools appointed pursuant to § 38-174.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_38-755_02_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“DCPS” means the District of Columbia Public Schools established by § 38-171.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
