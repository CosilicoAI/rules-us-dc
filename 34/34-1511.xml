      <akn:section eId="sec_34_1511">
        <akn:num>34-1511</akn:num>
        <akn:heading>Recovery for public purpose programs.</akn:heading>
<akn:subsection eId="subsec_34-1511_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_34-1511_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>All costs recoverable under this section may be recovered through a surcharge or other appropriate cost recovery mechanism to be determined by the Commission.</akn:p></akn:content>
    </akn:subsection>
