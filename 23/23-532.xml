      <akn:section eId="sec_23_532">
        <akn:num>23-532</akn:num>
        <akn:heading>Depositing of seized currency.</akn:heading>
<akn:subsection eId="subsec_23-532_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_23-532_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_23-532_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>Nothing in this subchapter shall apply to currency advanced to the Metropolitan Police Department, from appropriated funds for use in undercover police activities.</akn:p></akn:content>
