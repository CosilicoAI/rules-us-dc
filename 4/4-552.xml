      <akn:section eId="sec_4_552">
        <akn:num>4-552</akn:num>
        <akn:heading>Domestic Violence Hotline.</akn:heading>
<akn:subsection eId="subsec_4-552_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>The Office shall establish the Domestic Violence Hotline to provide assistance for victims and potential victims of domestic violence beginning October 1, 2014.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_4-552_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_4-552_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>The Office shall develop and implement an outreach campaign to educate District residents about the Hotline.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
