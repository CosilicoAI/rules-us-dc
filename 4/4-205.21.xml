      <akn:section eId="sec_4_205_21">
        <akn:num>4-205.21</akn:num>
        <akn:heading>Eligibility standards for children of unemployed parents.</akn:heading>
<akn:subsection eId="subsec_4-205_21_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Repealed.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_4-205_21_a_1">
      <akn:num>(a-1)</akn:num>
      <akn:content><akn:p>Repealed.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_4-205_21_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>Repealed.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_4-205_21_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>The parent who is the principal wage earner must be referred to job search, job readiness, or other work activities after application for TANF benefits.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
