        <akn:num>42-3173.07</akn:num>
        <akn:heading>Final determination of deteriorated structure.</akn:heading>
<akn:subsection eId="subsec_42-3173_07_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_42-3173_07_b">
      <akn:num>(b)</akn:num>
