      <akn:section eId="sec_29_1205_02">
        <akn:num>29-1205.02</akn:num>
        <akn:heading>Trustee powers.</akn:heading>
<akn:subsection eId="subsec_29-1205_02_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Powers conferred by the governing instrument;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_29-1205_02_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Except as limited by the governing instrument, any other powers necessary or convenient to carry out the activities and affairs of the statutory trust; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_29-1205_02_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>Other powers conferred by this chapter.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
