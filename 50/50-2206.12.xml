      <akn:section eId="sec_50_2206_12">
        <akn:num>50-2206.12</akn:num>
        <akn:heading>Driving under the influence of alcohol or a drug; commercial vehicle.</akn:heading>
<akn:subsection eId="subsec_50-2206_12_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>While the person is intoxicated; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_50-2206_12_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>While the person is under the influence of alcohol or any drug or any combination thereof.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
