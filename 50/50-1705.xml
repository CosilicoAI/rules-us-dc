      <akn:section eId="sec_50_1705">
        <akn:num>50-1705</akn:num>
        <akn:heading>Application of chapter.</akn:heading>
<akn:subsection eId="subsec_50-1705_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>This chapter shall apply to any person operating a motor vehicle in the District of Columbia.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_50-1705_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>This chapter shall apply to any motor vehicle that is used for personal use.</akn:p></akn:content>
    </akn:subsection>
