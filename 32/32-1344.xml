      <akn:section eId="sec_32_1344">
        <akn:num>32-1344</akn:num>
        <akn:heading>Penalties.</akn:heading>
<akn:subsection eId="subsec_32-1344_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>For employers that employ 11 to 30 employees, a fine of up to $1,000;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_32-1344_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>For employers that employ 31 to 99 employees, a fine of up to $2,500; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_32-1344_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>For employers that employ 100 or more employees, a fine of up to $5,000.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
    </akn:body>
