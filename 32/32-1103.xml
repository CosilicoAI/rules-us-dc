      <akn:section eId="sec_32_1103">
        <akn:num>32-1103</akn:num>
        <akn:heading>Duties of employer and employees.</akn:heading>
<akn:subsection eId="subsec_32-1103_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Each employer shall:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_32-1103_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>Each employee shall comply with all occupational safety and health rules promulgated and orders issued pursuant to this chapter which are applicable to the actions and conduct of the employee.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
