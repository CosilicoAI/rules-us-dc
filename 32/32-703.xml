      <akn:section eId="sec_32_703">
        <akn:num>32-703</akn:num>
        <akn:heading>Enforcement by civil action.</akn:heading>
<akn:subsection eId="subsec_32-703_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>A false statement in a declaration of domestic partnership or a false assertion of family membership; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_32-703_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>A failure to notify the Mayor or the employer of a change in the status of the domestic partnership or family membership.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
