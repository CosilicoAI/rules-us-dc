      <akn:section eId="sec_3_1101">
        <akn:num>3-1101</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_3-1101_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Commission” means the District of Columbia Residential, Commercial, and  Institutional Structures Fire Protection Study Commission established by § 3-1102.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_3-1101_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Committee” means the Interdepartmental Advisory Committee established by  § 3-1107.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_3-1101_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Council” means Council of the District of Columbia.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_3-1101_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>“Mayor” means the Mayor of the District of Columbia.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
