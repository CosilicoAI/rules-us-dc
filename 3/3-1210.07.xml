      <akn:section eId="sec_3_1210_07">
        <akn:num>3-1210.07</akn:num>
        <akn:heading>Criminal penalties.</akn:heading>
<akn:subsection eId="subsec_3-1210_07_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Any person who violates any provision of this chapter shall, upon conviction, be subject to imprisonment not to exceed 1 year, or a fine not to exceed $10,000, or both.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_3-1210_07_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>Any person who has been previously convicted under this chapter shall, upon conviction, be subject to imprisonment not to exceed 1 year, or a fine not to exceed $25,000, or both.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
