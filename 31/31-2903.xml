      <akn:section eId="sec_31_2903">
        <akn:num>31-2903</akn:num>
        <akn:heading>Applicability.</akn:heading>
<akn:subsection eId="subsec_31-2903_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>To any health benefit plan delivered or issued for delivery in the District more than 120 days after March 7, 1991; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-2903_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>To any health benefit plan renewed, amended, or reissued 120 days after March 7, 1991.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
