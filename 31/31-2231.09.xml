      <akn:section eId="sec_31_2231_09">
        <akn:num>31-2231.09</akn:num>
        <akn:heading>Stock operations and advisory board contracts.</akn:heading>
<akn:subsection eId="subsec_31-2231_09_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Agency company stock or other capital stock, benefit certificates, or shares in a common law corporation;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-2231_09_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Securities; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-2231_09_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>A special or advisory board contract or other contracts of any kind promising returns and profits.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
