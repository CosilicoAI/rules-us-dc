      <akn:section eId="sec_31_5051_04">
        <akn:num>31-5051.04</akn:num>
        <akn:heading>Termination and modification of coverage.</akn:heading>
<akn:subsection eId="subsec_31-5051_04_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Notwithstanding any other provision of law:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-5051_04_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-5051_04_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>Notice or correspondence required by this section or otherwise required by law may be sent on behalf of an insurer or vendor, as the case may be, by the supervising entity appointed by the insurer.</akn:p></akn:content>
    </akn:subsection>
