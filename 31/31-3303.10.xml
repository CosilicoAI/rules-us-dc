      <akn:section eId="sec_31_3303_10">
        <akn:num>31-3303.10</akn:num>
        <akn:heading>Exclusions.</akn:heading>
<akn:subsection eId="subsec_31-3303_10_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Any group health benefit plan for any plan year if, on the first day of such plan year, such plan has less than 2 participants who are current employees; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_31-3303_10_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Any health benefit plan for any of the excepted benefits.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
