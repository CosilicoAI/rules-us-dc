      <akn:section eId="sec_2_1402_71">
        <akn:num>2-1402.71</akn:num>
        <akn:heading>Prohibitions.</akn:heading>
<akn:subsection eId="subsec_2-1402_71_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>To fail or refuse to issue a policy of motor vehicle insurance;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1402_71_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>To fail or refuse to renew a policy of motor vehicle insurance; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1402_71_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>To cancel a policy of motor vehicle insurance.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
