      <akn:section eId="sec_2_1515_04a">
        <akn:num>2-1515.04a</akn:num>
        <akn:heading>Behavioral health screening and assessment requirements.</akn:heading>
<akn:subsection eId="subsec_2-1515_04a_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1515_04a_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>For the purposes of this section, the term “youth” means an individual under 18 years of age residing in the District and those individuals classified as committed youth in the custody of the Department who are 21 years of age or younger.</akn:p></akn:content>
    </akn:subsection>
