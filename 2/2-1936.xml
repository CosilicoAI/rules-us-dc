      <akn:section eId="sec_2_1936">
        <akn:num>2-1936</akn:num>
        <akn:heading>Phased implementation.</akn:heading>
<akn:subsection eId="subsec_2-1936_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>This subchapter shall apply on June 19, 2004 to:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1936_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>This subchapter shall apply as of October 1, 2004 to:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1936_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>This subchapter shall apply as of October 1, 2005, to:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1936_d">
      <akn:num>(d)</akn:num>
      <akn:content><akn:p>This subchapter shall apply as of October 1, 2006 to all covered entities.</akn:p></akn:content>
    </akn:subsection>
