      <akn:section eId="sec_2_1707">
        <akn:num>2-1707</akn:num>
        <akn:heading>Confidentiality safeguarded.</akn:heading>
<akn:subsection eId="subsec_2-1707_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>Any public record made confidential by law shall be so treated.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-1707_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>No provision of this chapter shall be construed to authorize or require the opening of any records ordered to be sealed by a court.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
