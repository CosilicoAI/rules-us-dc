      <akn:section eId="sec_2_358_03">
        <akn:num>2-358.03</akn:num>
        <akn:heading>Electronic inventory control system for surplus property.</akn:heading>
<akn:subsection eId="subsec_2-358_03_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>The date of the receipt of the surplus property;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-358_03_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>The agency or organization from which the surplus property was received;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-358_03_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>A description of the surplus property, including quantity and condition;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-358_03_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>A photograph of the surplus property; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_2-358_03_5">
      <akn:num>(5)</akn:num>
      <akn:content><akn:p>The estimated value of the surplus property.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
