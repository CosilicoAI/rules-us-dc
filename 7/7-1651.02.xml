      <akn:section eId="sec_7_1651_02">
        <akn:num>7-1651.02</akn:num>
        <akn:heading>Human papillomavirus public education campaign.</akn:heading>
<akn:subsection eId="subsec_7-1651_02_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>The connection between HPV and cervical cancer;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1651_02_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>The importance of protecting oneself against HPV infection;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1651_02_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>The value of screening for cervical cancer through regular pap tests; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1651_02_4">
      <akn:num>(4)</akn:num>
