      <akn:section eId="sec_7_651_08">
        <akn:num>7-651.08</akn:num>
        <akn:heading>Comfort care; alleviation of pain.</akn:heading>
<akn:subsection eId="subsec_7-651_08_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Clear the airway, excluding artificial ventilation, esophageal obturator airway, or advanced airway management;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-651_08_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Administer suction;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-651_08_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>Provide oxygen, excluding artificial ventilation, esophageal obturator airway, or advanced airway management;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-651_08_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>Provide pain medication;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-651_08_5">
      <akn:num>(5)</akn:num>
      <akn:content><akn:p>Control bleeding; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-651_08_6">
      <akn:num>(6)</akn:num>
      <akn:content><akn:p>Make comfort adjustments.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
    </akn:body>
