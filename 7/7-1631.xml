      <akn:section eId="sec_7_1631">
        <akn:num>7-1631</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_7-1631_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“AIDS” means acquired immune deficiency syndrome.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1631_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Department” means the Department of Health.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1631_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“HIV” means the human immunodeficiency virus.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1631_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>“Program” means the Senior HIV/AIDS Education and Outreach Program established by § 7-1632.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-1631_5">
      <akn:num>(5)</akn:num>
      <akn:content><akn:p>“Senior” means an individual 50 years of age or older.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
