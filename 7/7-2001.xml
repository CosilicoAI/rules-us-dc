      <akn:section eId="sec_7_2001">
        <akn:num>7-2001</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_7-2001_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>“Child” means “child” as defined in § 4-401(1).</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-2001_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>“Child development center” means “child development center” as defined in § 4-401(2).</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-2001_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>“Child development home” means “child development home” as defined in § 4-401(3).</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-2001_d">
      <akn:num>(d)</akn:num>
      <akn:content><akn:p>“Fund” means the Child Care Services Assistance Fund established by § 7-2002.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_7-2001_e">
      <akn:num>(e)</akn:num>
      <akn:content><akn:p>“Person” means an individual, partnership, association, or corporation.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
    </akn:body>
