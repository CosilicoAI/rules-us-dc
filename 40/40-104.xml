      <akn:section eId="sec_40_104">
        <akn:num>40-104</akn:num>
        <akn:heading>Application of proceeds of sale.</akn:heading>
<akn:subsection eId="subsec_40-104_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>To the expenses of such sales and the discharge of such lien;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_40-104_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>To payment of other liens, if any, in the order of their priority; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_40-104_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>To the owner of the property.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
