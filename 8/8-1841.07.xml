      <akn:section eId="sec_8_1841_07">
        <akn:num>8-1841.07</akn:num>
        <akn:heading>Care of animal.</akn:heading>
<akn:subsection eId="subsec_8-1841_07_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Full access to an enclosed shelter sufficient to protect the dog from wind, rain, excessive heat or cold, and disease; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_8-1841_07_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Continuous access to sufficient food and water.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
