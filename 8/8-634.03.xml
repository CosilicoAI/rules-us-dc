      <akn:section eId="sec_8_634_03">
        <akn:num>8-634.03</akn:num>
        <akn:heading>Access to information.</akn:heading>
<akn:subsection eId="subsec_8-634_03_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>The Mayor, upon reasonable notice, may require any person who has or may have responsive information to:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_8-634_03_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_8-634_03_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>This chapter shall not prevent or preclude the Mayor from securing access or obtaining information in any other lawful and reasonable manner, including by issuing a subpoena to compel the production of information.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
