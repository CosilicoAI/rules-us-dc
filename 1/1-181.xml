      <akn:section eId="sec_1_181">
        <akn:num>1-181</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_1-181_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Emancipation Day Parade” means the parade, and associated activities, including the provision of food, snacks, entertainment, and non-alcoholic beverages to the general public, participants, and District government employees, held to celebrate and commemorate District of Columbia Emancipation Day.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-181_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Fund” means the Emancipation Day Fund.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
