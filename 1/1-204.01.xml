      <akn:section eId="sec_1_204_01">
        <akn:num>1-204.01</akn:num>
        <akn:heading>Creation and membership.</akn:heading>
<akn:subsection eId="subsec_1-204_01_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>There is established a Council of the District of Columbia; and the members of the Council shall be elected by the registered qualified electors of the District.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-204_01_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-204_01_c">
      <akn:num>(c)</akn:num>
      <akn:content><akn:p>The Council may establish and select such other officers and employees as it deems necessary and appropriate to carry out the functions of the Council.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-204_01_d">
      <akn:num>(d)</akn:num>
      <akn:content><akn:p></akn:p></akn:content>
