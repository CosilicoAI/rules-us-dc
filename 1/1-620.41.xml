      <akn:section eId="sec_1_620_41">
        <akn:num>1-620.41</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_1-620_41_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Applicant” means an individual who has filed an application for employment with a public employer or who has filed an application or made a verbal request to serve in a volunteer position with a public employer.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-620_41_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Covered position” means a position in which a criminal background check is required by law.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-620_41_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Public employer” means the District government.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
