      <akn:section eId="sec_1_622_04">
        <akn:num>1-622.04</akn:num>
        <akn:heading>Definitions.</akn:heading>
<akn:subsection eId="subsec_1-622_04_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>“Annuitant” means:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-622_04_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>“Dependent child” includes:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-622_04_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>“Employee” means an individual first employed by the District after September 30, 1987.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-622_04_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>“Member of family” means:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-622_04_5">
      <akn:num>(5)</akn:num>
      <akn:content><akn:p>“Viatical settlement” means an irrevocable assignment of all an employee’s or former employee’s incidents of ownership in a life insurance policy.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
