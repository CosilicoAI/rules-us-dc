      <akn:section eId="sec_1_606_08">
        <akn:num>1-606.08</akn:num>
        <akn:heading>Attorney fees.</akn:heading>
<akn:subsection eId="subsec_1-606_08_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>The Hearing Examiner or the Arbitrator may require payment by the agency of reasonable attorney fees if the appellant is the prevailing party and payment is warranted in the interest of justice.</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-606_08_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>Expired.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
