      <akn:section eId="sec_1_325_224">
        <akn:num>1-325.224</akn:num>
        <akn:heading>Reporting requirements.</akn:heading>
<akn:subsection eId="subsec_1-325_224_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Detailed subgrantee data;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Performance measures and performance outcomes under each subgrant;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>The specific services provided under each subgrant;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>The entity providing the services, if one other than the subgrantee;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_5">
      <akn:num>(5)</akn:num>
      <akn:content><akn:p>The time period of delivery of the services;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_6">
      <akn:num>(6)</akn:num>
      <akn:content><akn:p>The type of service provided;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_7">
      <akn:num>(7)</akn:num>
      <akn:content><akn:p>The actual amount paid for the services; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-325_224_8">
      <akn:num>(8)</akn:num>
      <akn:content><akn:p>The amount of other expenditures under the subgrant, if any.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
