      <akn:section eId="sec_1_623_26">
        <akn:num>1-623.26</akn:num>
        <akn:heading>Subpoenas; oaths; examination of witnesses.</akn:heading>
<akn:subsection eId="subsec_1-623_26_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Issue subpoenas for and compel the attendance of witnesses within a radius of 100 miles of the District of Columbia;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-623_26_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Administer oaths;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-623_26_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>Examine witnesses; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-623_26_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>Require the production of books, papers, documents, and other evidence.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
