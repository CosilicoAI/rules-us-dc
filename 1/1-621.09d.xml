      <akn:section eId="sec_1_621_09d">
        <akn:num>1-621.09d</akn:num>
        <akn:heading>Preparation of studies; enrolled actuary.</akn:heading>
<akn:subsection eId="subsec_1-621_09d_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Direct the enrolled actuary engaged pursuant to §  1-621.09a to:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_1-621_09d_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Rebid its contract with an enrolled actuary no less frequently than every 5 years.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
