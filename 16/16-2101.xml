      <akn:section eId="sec_16_2101">
        <akn:num>16-2101</akn:num>
        <akn:heading>Definition of joint and several contracts.</akn:heading>
<akn:subsection eId="subsec_16-2101_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>the persons are partners or joint contractors;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-2101_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>the contract is under seal or not;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-2101_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>it is written or verbal; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-2101_4">
      <akn:num>(4)</akn:num>
      <akn:content><akn:p>it is expressed to be joint and several or not â€”</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
