        <akn:num>16-1006</akn:num>
        <akn:heading>Jurisdiction.</akn:heading>
<akn:subsection eId="subsec_16-1006_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>The petitioner resides, lives, works, or attends school in the District of Columbia:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-1006_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>The petitioner is under the legal custody of a District government agency; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-1006_3">
      <akn:num>(3)</akn:num>
