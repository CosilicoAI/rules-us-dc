      <akn:section eId="sec_16_4427">
        <akn:num>16-4427</akn:num>
        <akn:heading>Appeals.</akn:heading>
<akn:subsection eId="subsec_16-4427_a">
      <akn:num>(a)</akn:num>
      <akn:content><akn:p>An appeal may be taken from:</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_16-4427_b">
      <akn:num>(b)</akn:num>
      <akn:content><akn:p>An appeal under this section shall be taken as from an order or a judgment in a civil action.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
