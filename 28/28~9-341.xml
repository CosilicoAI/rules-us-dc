      <akn:section eId="sec_28:9_341">
        <akn:num>28:9-341</akn:num>
        <akn:heading>Bank’s rights and duties with respect to deposit account.</akn:heading>
<akn:subsection eId="subsec_28:9-341_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>The creation, attachment, or perfection of a security interest in the deposit account;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_28:9-341_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>The bank’s knowledge of the security interest; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_28:9-341_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>The bank’s receipt of instructions from the secured party.</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
