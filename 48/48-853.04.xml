      <akn:section eId="sec_48_853_04">
        <akn:num>48-853.04</akn:num>
        <akn:heading>Authority to access database.</akn:heading>
<akn:subsection eId="subsec_48-853_04_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Licensed, registered, or certified by a health occupations board; and</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_48-853_04_2">
      <akn:num>(2)</akn:num>
