        <akn:num>26-1120</akn:num>
        <akn:heading>Limitation on name of mortgage business.</akn:heading>
<akn:subsection eId="subsec_26-1120_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>A bank, trust company, or savings bank;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_26-1120_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>A savings and loan association;</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_26-1120_3">
      <akn:num>(3)</akn:num>
      <akn:content><akn:p>A credit union; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_26-1120_4">
      <akn:num>(4)</akn:num>
