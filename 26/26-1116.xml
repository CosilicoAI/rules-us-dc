      <akn:section eId="sec_26_1116">
        <akn:num>26-1116</akn:num>
        <akn:heading>Advertising.</akn:heading>
<akn:subsection eId="subsec_26-1116_1">
      <akn:num>(1)</akn:num>
      <akn:content><akn:p>Contains any false, misleading, or deceptive statement or representation; or</akn:p></akn:content>
    </akn:subsection><akn:subsection eId="subsec_26-1116_2">
      <akn:num>(2)</akn:num>
      <akn:content><akn:p>Identifies the mortgage lender, mortgage broker, mortgage loan originator, or loan officer by any name other than the name set forth on the license issued by the Superintendent [Commissioner].</akn:p></akn:content>
    </akn:subsection>
      </akn:section>
